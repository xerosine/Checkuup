        >
          <div
            class="hidden lg:block lg:w-1/2 lg:h-[554px] bg-[url(../images/contact-us.jpg)] bg-cover bg-position-[30%_center] rounded-2xl"
          >
            <!-- <img src="/images/contact-us.jpg" alt="" class=""> -->
          </div>
          <div
            class="max-w-[320px] md:w-[60%] md:max-w-[600px] lg:max-w-[450px] lg:w-1/2 mx-auto"
          >
            <form action="javascript:void(0)" method="post">
              <div class="mb-5">
                <label
                  for="name"
                  class="ml-1 font-medium mb-2 block text-[#ddd]"
                  >Name</label
                >
                <input
                  type="text"
                  id="name"
                  name="name"
                  class="py-2 px-2 rounded-lg bg-white text-[1.25rem] placeholder:text-[#737373] w-full focus-visible:outline-[#0098db]"
                  placeholder="Enter name here"
                />
              </div>
              <div class="mb-5">
                <label
                  for="email"
                  class="ml-1 font-medium mb-2 block text-[#ddd]"
                  >Email</label
                >
                <input
                  type="text"
                  id="email"
                  name="email"
                  class="py-2 px-2 rounded-lg bg-white text-[1.25rem] placeholder:text-[#737373] w-full focus-visible:outline-[#0098db]"
                  placeholder="Email Address"
                />
              </div>
              <div class="mb-5">
                <label
                  for="subject"
                  class="ml-1 font-medium mb-2 block text-[#ddd]"
                  >Subject</label
                >
                <select
                  name="subject"
                  required
                  class="py-2 px-2 rounded-lg bg-white text-black text-[1.25rem] invalid:text-[#737373] w-full focus-visible:outline-[#0098db]"
                >
                  <option selected disabled value="">Select Option</option>
                  <option value="generalInquiry">General Inquiry</option>
                  <option value="corporateWellness">Corporate Wellness</option>
                  <option value="partnerships">Partnerships</option>
                  <option value="media">Media</option>
                  <option value="technicalSupport">Technical Support</option>
                </select>
              </div>
              <div class="mb-5">
                <label
                  for="message"
                  class="ml-1 font-medium mb-2 block text-[#ddd]"
                  >Message</label
                >
                <textarea
                  id="message"
                  name="message"
                  class="py-2 px-2 rounded-lg bg-white text-[1.25rem] placeholder:text-[#737373] focus-visible:outline-[#0098db] h-[10rem] w-full"
                  placeholder="Enter Message here"
                ></textarea>
              </div>
              <div class="">
                <button
                  type="submit"
                  class="bg-primary text-white rounded-lg py-2 w-full font-bold text-[1.25rem]"
                >
                  Submit
                </button>
              </div>
            </form>
          </div>
        </div>
      </section>
