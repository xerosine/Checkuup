            "border-b-2",
            "md:border-b-4",
            "border-primary",
            "text-primary"
          );
          corpartButtons[0].classList.add("md:border-b-2");
          corpartButtons[1].classList.add(
            "text-primary",
            "border-b-2",
            "md:border-b-4",
            "border-primary",
            "text-primary"
          );
          corpwell.classList.remove("opacity-100", "z-5");
          partner.classList.add("opacity-100", "z-5");
          corpartSect.classList.replace("h-[1580px]", "h-[1530px]");
          corpartSect.classList.replace("md:h-[1830px]", "md:h-[1770px]");
          corpartSect.classList.replace("xl:h-[1630px]", "xl:h-[1900px]");
          // corpartSect.classList.add("h-[1470px]", "sm:h-[1510px]");
        }
      };

      corpartButtons.forEach((btn, index) => {
        btn.addEventListener("click", () => {
          corpartFn(index);
        });
      });

      //faq script
      const faqButtons = document.querySelectorAll(".faq-btn");
      const faqs = document.querySelectorAll(".faq");

      faqButtons.forEach((btn, index) => {
        btn.addEventListener("click", () => {
          let dropdownOpen = btn.classList.contains("rotate-180");
          if (dropdownOpen) {
            faqs[index].classList.remove(
              "h-[20rem]",
              "sm:h-[17rem]",
              "md:h-[17rem]",
              "xl:h-[14rem]"
            );
            btn.classList.remove("rotate-180");
          } else {
            faqs[index].classList.add(
              "h-[20rem]",
              "sm:h-[17rem]",
              "md:h-[17rem]",
              "xl:h-[14rem]"
            );
            btn.classList.add("rotate-180");
          }
        });
      });

      function openModal() {
        document.body.classList.add("overflow-hidden");
        document.getElementById("modal").classList.remove("hidden");
      }

      function closeModal() {
        document.body.classList.remove("overflow-hidden");
        document.getElementById("modal").classList.add("hidden");
      }

      const checkAgree = document.querySelector("#agree")
      console.log(checkAgree);
      
      checkAgree.onclick = (e) => {
        if (e.target.value === 'off') {
          e.target.value = 'on'
          joinForm.onsubmit = handleJoinForm
          document.querySelector("#joinBtn").classList.replace("bg-[#ddd]", "bg-[#007aff]")
          document.querySelector("#joinBtn").classList.add("text-white", "cursor-pointer")
        } else {
          e.target.value = 'off'
          joinForm.onsubmit = null
          document.querySelector("#joinBtn").classList.replace("bg-[#007aff]", "bg-[#ddd]")
          document.querySelector("#joinBtn").classList.remove("text-white", "cursor-pointer")
        }
        console.log(e.target.value);
      }

      const contactForm = document.querySelector("form");

      const handleContactForm = async (e) => {
        let status
        const formData = new FormData(contactForm);
        const data = Object.fromEntries(formData.entries());
        for (key in data) {
          if (!data[key]) {
            showMessage(
              `The ${key} field is required!`,
              "border-red-500",
              "bg-red-500"
            );
          }
        }
        await fetch("contact-us", {
          method: "post",
          body: formData,
        })
          .then((res) => {
            status = res.status
            return res.json()
          })
          .then(data => {
            if (status === 200) {
            showMessage(data.message, "border-green-500", "bg-green-500")
          } else {
            showMessage(data.message, "border-red-500", "bg-red-500")
          }
          })
          .catch((err) => {
            const message = err.message || "Something went wrong";
            showMessage(message, "border-red-500", "bg-red-500");
          });
      }

      contactForm.addEventListener("submit", handleContactForm);
      
      const joinForm = document.querySelector("#joinForm");

      const handleJoinForm = async (e) => {
        let status
        const formData = new FormData(joinForm);
        const data = Object.fromEntries(formData.entries());
        for (key in data) {
          if (!data[key]) {
            showMessage(
              `The ${key} field is required!`,
              "border-red-500",
              "bg-red-500"
            );
          }
        }
        await fetch("join-the-movement", {
          method: "post",
          body: formData,
        })
          .then((res) => {
            status = res.status
            return res.json()
          })
          .then(data => {
            console.log(data);
            console.log(status);
            
            if (status === 200) {
            showMessage(data.message, "border-green-500", "bg-green-500")
          } else {
            showMessage(data.message, "border-red-500", "bg-red-500")
          }
          })
          .catch((err) => {
            const message = err.message || "Something went wrong";
            showMessage(message, "border-red-500", "bg-red-500");
          });
      };

      const messageBoxCont = document.querySelector("#messageBox");
      const messageBox = document.querySelector("#messageBox p");
      const messageTimer = document.querySelector("#messageTimer");
      function showMessage(message, border, timerColor) {
        messageBox.textContent = message;
        messageBoxCont.classList.add(border, "-translate-x-75", "opacity-100");
        messageTimer.classList.add(timerColor, "w-full");
        setTimeout(() => {
          messageBoxCont.classList.remove(border, "-translate-x-75", "opacity-100");
          messageTimer.classList.remove(timerColor, "w-full");
        }, 5000);
      }
    </script>
  </body>
</html>
